{
  "name": "citystate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "css": "node-sass --output-style compressed -o public/css/dist public/css/src/style.scss",
    "postcss": "postcss -u autoprefixer -r public/css/dist/*",
    "build": "npm run css && npm run postcss",
    "watch:all": "echo 'test'",
    "start:dev": "npm run build && parallelshell 'npm run watch:all' 'node ./bin www'",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "aws-sdk": "^2.2.35",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "node-sass": "^3.4.2",
    "parallelshell": "^2.0.0",
    "postcss": "^5.0.16",
    "postcss-cli": "^2.5.1"
  }
}
